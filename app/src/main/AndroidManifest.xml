<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moboassignment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoboAssignment">
        <activity android:name=".NotificationActivity"></activity>
        <activity android:name=".NotifyActivity" />
        <activity android:name=".CallActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".OpenActivity" />
        <activity android:name=".ViewProductActivity" />
        <activity android:name=".AddProductActivity" />
        <activity android:name=".MenusActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>